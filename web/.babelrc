{
  "extends": "../babel.config.js",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead",
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "src": "./src"
        }
      }
    ]
  ]
}
